{
  "name": "Scarborough",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Music Hack Day 2014",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [],
  "background": {
    "persistent": false,
    "scripts": ["gracenote_get_id.js", "gracenote_get_playlist.js"]
  },
