{
  "name": "Scarborough",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Music Hack Day 2014",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-1.7.min.js", "contentscript.js"]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": ["jquery-1.7.min.js", "bg.js"]
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Scarborough",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://*.web.cddbp.net/"
  ],
  "options_page": "options.html"
}
