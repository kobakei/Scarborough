{
  "name": "Scarborough",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Music Hack Day 2014",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [],
  "background": {
    "persistent": false,
    "scripts": [
      "card.js",
      "spotify_get_id.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Scarborough",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "alarms",
    "notifications",
    "https://*.web.cddbp.net/"
  ],
  "web_accessible_resources": [
    "icon128.png"
  ],
  "options_page": "options.html"
}
